#include <stdio.h>
#include <string.h>
int my_strlen(const char *str);
int main()
{
    char str[100];
    printf("Enter the string :");
    scanf("%s",str);
    int ret=my_strlen(str);
    printf("String length is %d",ret);
}
int my_strlen(const char *str)
{
    int count=0;
    while(*str != '\0')
    {
        count=count+1;
        str++;
        
    }
    return count;
}
